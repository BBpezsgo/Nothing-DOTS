    flex-shrink: 0;
    flex-grow: 1;
    flex-basis: 100px;
    display: flex;
    border-left-color: rgb(0, 0, 0);
    border-right-color: rgb(0, 0, 0);
    border-top-color: rgb(0, 0, 0);
    border-bottom-color: rgb(0, 0, 0);
    border-top-width: 2px;
    border-right-width: 2px;
    border-bottom-width: 2px;
    border-left-width: 2px;
}

.terminal:focus {
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-top-width: 2px;
    border-right-width: 2px;
    border-bottom-width: 2px;
    border-left-width: 2px;
}

.diagnostics-item {
    border-top-width: 1px;
    border-right-width: 1px;
    border-bottom-width: 1px;
    border-left-width: 1px;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
    border-left-color: rgb(26, 26, 26);
    border-right-color: rgb(26, 26, 26);
    border-top-color: rgb(26, 26, 26);
    border-bottom-color: rgb(26, 26, 26);
    background-color: rgb(56, 56, 56);
}

.diagnostics-item > .unity-foldout > Toggle {
    margin-top: 0;
    margin-right: 0;
    margin-left: 0;
    margin-bottom: 0;
}

.diagnostics-item > .unity-foldout > .unity-foldout__content {
    margin-left: 0;
}

.diagnostics-item > .unity-foldout > .unity-foldout__toggle > .unity-foldout__input > .unity-foldout__text {
    padding-top: 0;
    padding-right: 0;
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    padding-bottom: 0;
    padding-left: 0;
}

.diagnostics-item > .unity-foldout > .unity-foldout__toggle > .unity-foldout__input > #unity-checkmark {
    -unity-background-image-tint-color: rgb(209, 209, 209);
    width: 14px;
    height: 14px;
}

.chat-message {
    transition-property: all, opacity;
    transition-duration: 0s, 1s;
    transition-timing-function: ease, ease;
    transition-delay: 0s, 0s;
    opacity: 1;
}

.chat-message.old {
    opacity: 0;
}

.chat-message.very-old {
    display: none;
}

.show .chat-message {
    opacity: 1;
    display: flex;
    transition-duration: 0s, 0s;
}
