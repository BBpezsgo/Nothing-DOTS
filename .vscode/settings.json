{
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.vs": true,
        "**/.gitmodules": true,
        "**/.vsconfig": true,
        "**/*.booproj": true,
        "**/*.pidb": true,
        "**/*.suo": true,
        "**/*.user": true,
        "**/*.userprefs": true,
        "**/*.unityproj": true,
        "**/*.dll": true,
        "**/*.exe": true,
        "**/*.pdf": true,
        "**/*.mid": true,
        "**/*.midi": true,
        "**/*.wav": true,
        "**/*.gif": true,
        "**/*.ico": true,
        "**/*.jpg": true,
        "**/*.jpeg": true,
        "**/*.png": true,
        "**/*.psd": true,
        "**/*.tga": true,
        "**/*.tif": true,
        "**/*.tiff": true,
        "**/*.3ds": true,
        "**/*.3DS": true,
        "**/*.fbx": true,
        "**/*.FBX": true,
        "**/*.lxo": true,
        "**/*.LXO": true,
        "**/*.ma": true,
        "**/*.MA": true,
        "**/*.obj": true,
        "**/*.OBJ": true,
        "**/*.asset": true,
        "**/*.cubemap": true,
        "**/*.flare": true,
        "**/*.mat": true,
        "**/*.meta": true,
        "**/*.prefab": false,
        "**/*.unity": true,
        "build/": true,
        "Build/": true,
        "Library/": true,
        "library/": true,
        "obj/": true,
        "Obj/": true,
        "Logs/": true,
        "logs/": true,
        "ProjectSettings/": true,
        "UserSettings/": true,
        "temp/": true,
        "Temp/": true
    },
    "dotnet.defaultSolution": "Nothing-DOTS.sln",
    "files.autoSave": "off",
    "vstuc.refreshOnSave": true,
    "vstuc.displayBackgroundProcesses": true,
    "omnisharp.useModernNet": false,
    "material-icon-theme.folders.associations": {
        "Netcode": "connection",
        "Materials": "shader",
        "Systems": "core",
        "UI Toolkit": "ui",
        "Input Settings": "console",
        "BasicStructs": "utils",
        "Authoring": "interface",
        "Mono": "class",
        "CodeFiles": "scripts",
        "Scenes": "environment",
        "SubScenes": "environment",
        "ThirdParty": "packages",
        "Prefabs": "container",
    },
    "material-icon-theme.files.associations": {
        "*.uxml": "xml",
        "*.tss": "css",
        "*.uss": "css"
    }
}
